{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "noob-blocks/heading",
	"version": "1.0.0",
	"title": "Heading",
	"category": "widgets",
	"description": "Heading block for your webpages.",
	"example": {},
	"attributes": {
		"blockClass": {
			"type": "string"
		},
		"blockStyle": {
			"type": "string"
		},
		"content": {
			"type": "string"
		},
		"htmlTag": {
			"type": "string",
			"default": "h2"
		},
		"alignment": {
			"type": "string"
		},
		"textColor": {
			"type": "string"
		},
		"typography": {
			"type": "object"
		},
		"textStroke": {
			"type": "object"
		},
		"textShadow": {
			"type": "object"
		},
		"blendMode": {
			"type": "string"
		}
	},
	"supports": {
		"html": false,
		"anchor": true,
		"align": [
			"full",
			"wide"
		]
	},
	"textdomain": "noob-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}